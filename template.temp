<html lang="en"><head>

  <!-- Basic Page Needs -->
  <meta charset="utf-8">
  <title>Alexandre Araujo</title>
  <meta name="description" content="">
  <meta name="author" content="">

  <!-- Mobile Specific Metas -->
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <!-- FONT -->
  <link href="//fonts.googleapis.com/css?family=Raleway:400,300,600" rel="stylesheet" type="text/css">

  <!-- CSS -->
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css">
  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.3.1/css/all.css">
  <link rel="stylesheet" href="https://cdn.rawgit.com/jpswalsh/academicons/master/css/academicons.min.css">
  <link rel="stylesheet" href="static/css/skeleton.css">
  <link rel="stylesheet" href="static/css/tooltips.css">
  <link rel="stylesheet" href="static/css/highlight.css">
  <link rel="stylesheet" href="static/css/custom.css">

  <!-- Scripts -->
  <script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/9.12.0/highlight.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/clipboard.js/2.0.0/clipboard.min.js"></script>
  <script src="static/js/tooltips.js"></script>

<body class="code-snippets-visible">

  <!-- Primary Page Layout -->
  <section class="header">

    <div class="container">
      <div class="row">
        <div class="col-lg-3">
          <img class="photo" src="photo.jpg">
        </div>
        <div class="col-lg-9">
          <h2 class="title">Alexandre Araujo</h2>
          <h6>alexandre.araujo<img src="static/img/at-sign-icon.png" height="10" width="10">inria.fr</h6>
        </div>
      </div>
      <div class="row">
        <p style="font-style: italic; margin-bottom:30px; margin-top: 30px; font-size: small">Last update October 2021</p>
      </div>

      <div class="row" style="text-align: justify;">
        <span>{{ description }}</span>
      </div>

      <div class="row" style="margin-bottom: 25px; margin-top: 25px;">
        <div class="col-3">
          <a style="margin-right:20px" href="https://scholar.google.fr/citations?user=wsu61VYAAAAJ">Google Scholar</a>
        </div>
        <div class="col-3">
          <a href="resume.pdf">Resume</a>
        </div>
      </div>

    </div>
  </section>


  <div class="container">

    <div class="row">
      <span class="docs-header">Research</span>
    </div>

    {% for paper in papers %}
    <div class="row">
      <div class="article">
        <div class="row">
          <span class="strong">{{ paper.title }}</span>
        </div>
        <div class="row">
          <span>{{ paper.authors }}</span>
        </div>
        <div class="row">
          <span>{{ paper.venue }} • {{ paper.year }}</span>
        </div>
        <div class="article-links">
          <a class="strong" href="{{ paper.url }}">paper</a> •
          {% if paper.code is not none %}
            <a class="strong" href="{{ paper.code }}">code</a> •
          {% endif %}
          <a class="strong" href="static/bibtex/{{ paper.bibtex }}.txt">bibtex</a>
        </div>
      </div>
    </div>
    {% endfor %}

  </div>


</body>

<!-- End Document -->

